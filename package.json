{
  "name": "power-horse-engine",
  "version": "0.0.0-development",
  "author": "Olanzo James <lanzoinc@gmail.com>",
  "license": "MIT",
  "release": {
    "branches": ["main"],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      ["@semantic-release/changelog",{
          "changelogFile": "CHANGELOG.md"
        }
      ],
      ["@semantic-release/github", {
        "assets": [
          {"path": "PowerHorseEngine.rbxm", "label": "PowerHorseEngine.rbxm"}
        ]
      }]
    ]
  },
  "scripts": {
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lanzoinc/PowerHorseEngine.git"
  },
  "devDependencies": {
    "semantic-release": "^19.0.5"
  },
  "publishConfig": {
    "access": "public"
  }
}
